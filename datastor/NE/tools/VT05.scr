# VERMONT
	STATEAB=VT
	 #State land area 23957 km^2
        STATEAREA=23957
	STATECNT=$(awk 'BEGIN {FS=OFS="|"} $4~/Vermont/&&$19~/ /&&$24!~/sp\./' c:/Lichen/datastor/NE/VT/VTdfbn|wc -l)
