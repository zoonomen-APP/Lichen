        STATECNTSD=$(awk 'BEGIN {FS=OFS="|"} $4~/Vermont/&&$19~/ /&&$24!~/sp\./&&$98~/Year/' c:/Lichen/datastor/NE/VT/VTdfbn|wc -l)
