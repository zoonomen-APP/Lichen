                echo --- >> c:/Lichen/datastor/NE/VT/00AAVermontx.txt
                echo  Vermont land area =  $STATEAREA km^2 >> c:/Lichen/datastor/NE/VT/00AAVermontx.txt
                echo -n $STATEAB State count density \(STATECNT/STATEAREA\*100 km^2\) : \  >> c:/Lichen/datastor/NE/VT/00AAVermontx.txt
                echo -n $STATECNT/$STATEAREA  = \   >> c:/Lichen/datastor/NE/VT/00AAVermontx.txt
                awk "BEGIN {print ($STATECNT/$STATEAREA)*100 }"  >> c:/Lichen/datastor/NE/VT/00AAVermontx.txt
                echo >> c:/Lichen/datastor/NE/VT/00AAVermontx.txt
                echo --- >> c:/Lichen/datastor/NE/VT/00AAVermontx.txt

